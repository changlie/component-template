{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "> 1%",
          "iOS > 7",
          "Android >= 4.0",
          "last 2 versions",
          "not ie <= 8"
        ]
      }
    }],
    "stage-2"
  ],
  "comments": false,
  "env": {
    "development": {
      "plugins": [
        ["transform-runtime", {
          "helpers": true,
          "polyfill": true,
          "regenerator": true
        }],
        ["transform-strict-mode", {
          "strict": false
        }],
        "dynamic-import-webpack"
      ]
    },
    "production": {
      "plugins": [
        ["transform-runtime", {
          "helpers": false,
          "polyfill": true,
          "regenerator": true
        }],
        ["transform-strict-mode", {
          "strict": false
        }],
      ]
    }
  }
}
